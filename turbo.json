{
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**"],
      "dependsOn": ["^build"]
    },
    "docs#build": {
      "cache": false,
      "dependsOn": [
        "^build",
        "$NEXT_PUBLIC_API_URL",
        "$NEXT_PUBLIC_ACCESS_URL"
      ],
      "outputs": ["dist/**", ".next/**"]
    },
    "storybook#run": {
      "cache": false
    },
    "storybook#build-storybook": {
      "cache": false
    },
    "storybook#chromatic": {},
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
